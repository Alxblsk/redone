{
  "name": "alxblsk-redone",
  "description": "Personal blog",
  "version": "1.0.1",
  "dependencies": {
    "classnames": "^2.3.2",
    "contentful": "^10.1.3",
    "gatsby": "^5.9.0",
    "gatsby-plugin-feed": "^5.9.0",
    "gatsby-plugin-gatsby-cloud": "^5.9.0",
    "gatsby-plugin-google-analytics": "^5.9.0",
    "gatsby-plugin-image": "^3.9.0",
    "gatsby-plugin-minify-classnames": "^5.0.0",
    "gatsby-plugin-react-helmet": "^6.9.0",
    "gatsby-plugin-sharp": "^5.9.0",
    "gatsby-plugin-sitemap": "^6.9.0",
    "gatsby-plugin-typescript": "^5.9.0",
    "gatsby-remark-prismjs": "^7.9.0",
    "gatsby-source-contentful": "^8.9.1",
    "gatsby-transformer-remark": "^6.9.0",
    "gatsby-transformer-sharp": "^5.9.0",
    "lodash": "^4.17.21",
    "prismjs": "^1.29.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-helmet": "^6.1.0",
    "react-schemaorg": "^2.0.0",
    "schema-dts": "^1.1.2",
    "sharp": "^0.32.1"
  },
  "resolutions": {
    "@hapi/joi": "https://github.com/hapijs/joi",
    "sharp": "^0.32.1",
    "gatsby-plugin-gatsby-cloud": "^5.9.0"
  },
  "engines": {
    "npm": ">=9.0.0",
    "node": ">=18.0.0",
    "yarn": ">=3.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.39.0",
    "eslint-plugin-react": "^7.32.2",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "yargs-parser": "^21.1.1"
  },
  "homepage": "https://belski.dev",
  "keywords": [
    "gatsby",
    "contentful",
    "netlify",
    "blog"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alxblsk/redone.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "dev": "npm run develop",
    "develop": "gatsby develop -p 4400",
    "clean": "gatsby clean",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'bin/*.js'",
    "build": "gatsby build",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix *.js bin/*.js",
    "start": "npm run develop",
    "netlify-build": "sed -i \"s|BLD_TKN|${API_WRITE_KEY}|g; s|BLD_HST|${REDONE_VOTE_HOST}|g; s|BLD_PRFX|${REDONE_VOTE_PREFIX}|g\" static/netlify.toml"
  },
  "dependenciesMeta": {
    "gatsby-transformer-remark@6.9.0": {
      "unplugged": true
    }
  }
}
