{
  "name": "alxblsk-redone",
  "description": "Personal blog",
  "version": "1.0.1",
  "dependencies": {
    "classnames": "^2.3.1",
    "contentful": "^7.14.4",
    "disqus-react": "^1.0.8",
    "gatsby-image": "^2.3.4",
    "gatsby-plugin-feed": "^2.5.3",
    "gatsby-plugin-google-analytics": "^2.3.1",
    "gatsby-plugin-minify-classnames": "^0.2.0",
    "gatsby-plugin-react-helmet": "^3.2.4",
    "gatsby-plugin-sharp": "^2.5.6",
    "gatsby-plugin-sitemap": "^2.4.3",
    "gatsby-plugin-typescript": "^2.4.3",
    "gatsby-remark-prismjs": "^3.5.1",
    "gatsby-source-contentful": "^2.2.9",
    "gatsby-transformer-remark": "^4.0.0",
    "gatsby-transformer-sharp": "^2.4.6",
    "joi": "^17.9.2",
    "lodash": "^4.17.15",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-schemaorg": "^1.0.2",
    "schema-dts": "^0.6.0",
    "sharp": "^0.32.0"
  },
  "resolutions": {
    "sharp": "^0.32.0"
  },
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=16.0.0 <17.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "gatsby": "^2.24.54",
    "gh-pages": "^2.2.0",
    "http-proxy-middleware": "^2.0.1",
    "husky": "^4.2.5",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "yargs-parser": "^18.1.3"
  },
  "homepage": "https://belski.dev",
  "keywords": [
    "gatsby",
    "contentful",
    "netlify",
    "blog"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alxblsk/redone.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "dev": "npm run develop",
    "develop": "gatsby develop -p 4400",
    "clean": "gatsby clean",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'bin/*.js'",
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix *.js bin/*.js",
    "start": "npm run develop",
    "heroku-postbuild": "gatsby build",
    "netlify-build": "sed -i \"s|BLD_TKN|${API_WRITE_KEY}|g; s|BLD_HST|${REDONE_VOTE_HOST}|g; s|BLD_PRFX|${REDONE_VOTE_PREFIX}|g\" static/netlify.toml"
  },
  "dependenciesMeta": {
    "gatsby-transformer-remark@6.9.0": {
      "unplugged": true
    }
  }
}
